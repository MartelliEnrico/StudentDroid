<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.martelli.enrico.studentdroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:logo="@drawable/ic_action_bar"
        android:theme="@style/Theme.Holostudent" >
        <activity
            android:name="me.martelli.enrico.studentdroid.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.martelli.enrico.studentdroid.MateriaActivity"
            android:label="@string/title_activity_lesson_recap"
            android:parentActivityName="me.martelli.enrico.studentdroid.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".AddHomeworkActivity"
            android:label="@string/title_activity_add_homework" >
        </activity>
    </application>

</manifest>
